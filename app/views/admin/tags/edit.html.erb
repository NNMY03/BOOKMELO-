<div class="container mt-5 pb-5 d-flex justify-content-center">
  <div class="form-group text-center">
　　<h3>タグ編集</h3>

      <%= form_with model: @tag, url: admin_tag_path, method: :patch do |f| %>
      <div class= 'mx-auto mt-5 mb-5'>
        <td><%= f.label :"タグ名", class: "mr-3" %></td>
        <td><%= f.text_field :name, class: "mr-5" %></td>
        <td><%= f.submit "変更を保存", class: "btn btn-primary" %></td>
      <% end %>
      </div>